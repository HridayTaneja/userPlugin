{
    "name": "integrateideas/user",
    "description": "Integrateideas/User plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "homepage": "https://github.com/integrateideas/userPlugin",
    "authors": [{

        "name": "Integrateideas",
        "role": "Author",
        "homepage": "https://github.com/integrateideas"
    }],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0",
        "hybridauth/hybridauth" : "^2.9"
    },
    "repositories": [{

        "type": "vcs",
        "url": "https://github.com/integrateideas/userPlugin"
    }],
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Integrateideas\\User\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Integrateideas\\User\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
